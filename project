	["SUSPECT"] = {
		["CUFF"] = {
			["CUFF"] = function() command_cuff(target_id) end,
			["UNCUFF"] = function() command_uncuff(target_id) end
		},
		["HOLD"] = function() command_hold(target_id) end,
