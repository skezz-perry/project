local gov_delay          = 0
local rpweaponid         = 1
local speed              = 0.3
local flymode            = false
local autopass           = false
local follow_state       = false
local timerActive        = false
local openBarrier        = false
local successful_login   = false
local player_status      = 0
local global_patrol
local player_a_status    = 0
local show_menu          = imgui.ImBool(false)
