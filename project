local text_cmd           = imgui.ImBuffer(9999)
local editor             = imgui.ImBool(false)

function main()
	if not isSampLoaded() or not isSampfuncsLoaded() then return end
	while not isSampAvailable() do wait(100) end

	autoUpdate("https://raw.githubusercontent.com/skezz-perry/project/master/version", "https://raw.githubusercontent.com/skezz-perry/project/master/project")

	sampRegisterChatCommand("helpimage", function()
		chat("Если при просмотре инвентаря появляются элементы с текстом {00CC66}ERROR{} нужно:")
		chat("1. Перейти по ссылке {00CC66}https://yadi.sk/d/Pc572dKGiE178g{} (скопирована в буффер-обмена).")
		chat("2. Скачать архив и разархивировать папку {00CC66}image{} по пути:")
		chat("{FFCD00}" .. getWorkingDirectory() .. "\\helper")
		chat("3. Перезагрузить скрипт через меню /mh или сочетанием клавиш CTRL + R.")
		setClipboardText("https://yadi.sk/d/Pc572dKGiE178g")
	end)
	 
	math.randomseed(os.time())
	 
	for name in pairs(scriptCommands.default) do
		if ini.defaultCommands[name] == nil then
			ini.defaultCommands[name] = true
		else
			scriptCommands.default[name][1] = ini.defaultCommands[name]
		end
	end 

	local numberCommands, registerCommands = 0, 0
	for command, _function in pairs(scriptCommands.default) do
		numberCommands = numberCommands + 1
		if not sampIsChatCommandDefined(command) then
			if _function[1] then
				if type(_G[_function[2]]) == ("function") then
					registerCommands = registerCommands + 1
					sampRegisterChatCommand(command, _G[_function[2]])
				end
			end
		end
	end
	print(("Во время подготовки было найдено %d стандартных команд(-ы) в скрипте, из них было зарегистрировано %d."):format(numberCommands, registerCommands))

	local numberCommands, registerCommands = 0, 0
	for name, value in pairs(ini.customCommands) do
		numberCommands = numberCommands + 1
		if not sampIsChatCommandDefined(name) then
			if value then
				local nameFile = string.format("moonloader//helper//commands//%s.txt", name)
				if doesFileExist(nameFile) then
					registerCommands = registerCommands + 1
					sampRegisterChatCommand(name, function(input)
						process_command(name, input)
					end)
				end
			end
		end
	end
	print(("Во время подготовки было найдено %d пользовательских команд(-ы) в скрипте, из них было зарегистрировано %d."):format(numberCommands, registerCommands))
	
	--[[memory.setuint8(7634870, 1) 
	memory.setuint8(7635034, 1)
	memory.fill(7623723, 144, 8)
	memory.fill(5499528, 144, 6)]]
	
	lua_thread.create(strobe)
	lua_thread.create(rpweapon)
	lua_thread.create(show_flymode)
	-- lua_thread.create(logotyp)
	-- lua_thread.create(speedometr)
	-- lua_thread.create(notifications)
	lua_thread.create(renderPlayerText)
	lua_thread.create(dynamicTime)
	lua_thread.create(suspectBar)
	
	apply_custom_style()
	register_custom_hotkey()
	add_new_hotkey()
	
	while true do
		wait(0)
		
		if ini.set.halfrpweapon and ini.set.rpweapon then
			if wasKeyPressed(KEY.VK_RBUTTON) then
				if lweap ~= getCurrentCharWeapon(playerPed) then
					if not isSampfuncsConsoleActive() and not sampIsChatInputActive() and not sampIsDialogActive() then
						if getCurrentCharWeapon(playerPed) ~= 0 then
							lua_thread.create(function()
								local result, rp = sampWeaponScene(getCurrentCharWeapon(playerPed))
								if result then
									wait(350)
									lweap = getCurrentCharWeapon(playerPed)
									sampSendChat(string.format("/me %s", rp))
									
									while lweap == getCurrentCharWeapon(playerPed) do wait(0) end
									
									wait(800)
									
									if (lweap ~= getCurrentCharWeapon(playerPed)) then
										local aresult, arp = sampWeaponSceneOff(lweap)
										if aresult then
											sampSendChat(string.format("/me %s", arp))
											lweap = -1
										end
									end
								end
							end)
						end
					end
				end
			end
			
			if wasKeyPressed(KEY.VK_R) and isKeyDown(VK_MENU) then
				if lweap ~= getCurrentCharWeapon(playerPed) then
					if not isSampfuncsConsoleActive() and not sampIsChatInputActive() and not sampIsDialogActive() then
						if getCurrentCharWeapon(playerPed) ~= 0 then
							lua_thread.create(function()
								local result, rp = sampWeaponScene(getCurrentCharWeapon(playerPed))
								if result then
									wait(350)
									lweap = getCurrentCharWeapon(playerPed)
									sampSendChat(string.format("/me %s", rp))
									
									while lweap == getCurrentCharWeapon(playerPed) do wait(0) end
									
									wait(800)
									
									if (lweap ~= getCurrentCharWeapon(playerPed)) then
										local aresult, arp = sampWeaponSceneOff(lweap)
										if aresult then
											sampSendChat(string.format("/me %s", arp))
											lweap = -1
										end
									end
								end
							end)
						end
					end
				end
			end
		end
 
		if isKeyDown(VK_RBUTTON) then
			local playerId, ped = sampGetPlayerIdByTarget()
			if playerId then
				if target_id ~= playerId then
					target_id = playerId
					if ini.set.smarker then
						if lastBlipChar then removeBlip(lastBlipChar) end
						if sampGetPlayerColor(target_id) ~= 2236962 then
							lastBlipChar = addBlipForChar(ped)
							changeBlipColour(lastBlipChar, "0x" .. sampGetColorByPlayer(id) .. "FF")
						end
					end
				end
			end
		end
		
		if lastBlipChar then
			if target_id then
				if isPlayerConnected(target_id) then
					if sampGetDistanceToPlayer(target_id) > 8.5 then
						removeBlip(lastBlipChar)
						target_id = -1
					end
				end
			else removeBlip(lastBlipChar) end
		end
		
		if wasKeyPressed(KEY.VK_H) and isKeyDown(VK_RBUTTON) then
			if ini.set.strobes then boolStrobes = not boolStrobes end
		end

		if show_menu.v or fastMenu.v or show_suspect.v or show_uk_list.v or show_ticket.v or show_ak_list.v or show_notification.v or show_server.v then
			imgui.Process = true
		else
			imgui.Process = false
		end
		
		--[[if ini.set.streetlight then
			local hour = getTimeOfDay() 
			if hour >= ini.set.slhour or hour <= ini.set.mslhour then 
				local x, y, z = getCharCoordinates(playerPed)
				for _, object in ipairs(mad.get_all_objects(x, y, z, 300)) do
					local model = getObjectModel(object)
					
					if getObjectHealth(object) > 0  and isObjectOnScreen(object) then
						if model == 1226 then
							local matrix = mad.get_object_matrix(object)
							if matrix then
								local objx, objy, objz = matrix:get_coords_with_offset(-1.3, 0.165, 3.69)
								local ground_z = getGroundZFor3dCoord(objx, objy, objz)
								mad.draw_spotlight(objx, objy, objz, objx, objy, ground_z, 0.065, 27)
							end
						elseif model == 1297 then
							local matrix = mad.get_object_matrix(object) 
							if matrix then
								local objx, objy, objz = matrix:get_coords_with_offset(-0.5, 0, 3.1)
								local ground_z = getGroundZFor3dCoord(objx, objy, objz)
								mad.draw_spotlight(objx, objy, objz, objx, objy, ground_z, 0.065, 27)
							end
						end
					end
				end
			end
		end]]
		 
		if successful_login then
			if sampGetCurrentServerName() ~= "SA-MP" then
				local _, id = sampGetPlayerIdByCharHandle(playerPed) 
				local name = sampGetPlayerName(id)
				local text = string.format("-\n(id %d ~ rang %d) %s successfully logged in as a user.\n-", id, player_status, name)
				sendGroup(text, true)
				successful_login = false
				attempToGetFileAndDir()
			end
		end
		
		--[[if ini.set.userrangname then
			if type(userList) == "table" then
				if not wasCreate3dLabel then wasCreate3dLabel = {} end
				
				for index, value in pairs(userList) do
					local playerId = sampGetPlayerIdByNickname(index)
					
					local result, ped = sampGetCharHandleBySampPlayerId(playerId)
					if result and not wasCreate3dLabel[index] then
						local text = ("%s%s"):format(value.color, u8:decode(value.rang))
						wasCreate3dLabel[index] = {sampCreate3dText(text, 0xFFFFFFFF, 0.0, 0.0, -0.7, 3.0, false, playerId, -1), playerId}
					end
					
					if not result and wasCreate3dLabel[index] then
						sampDestroy3dText(wasCreate3dLabel[index])
						wasCreate3dLabel[index] = nil
					end
				end
			end
		end--]]
		
		if flymode then
			if not sampIsChatInputActive() and not isSampfuncsConsoleActive() then
				--[[local angle = getCameraHeading()
				
				if isKeyDown(KEY.VK_W) then
					fci.position.x = fci.position.x + fci.speed * math.sin(math.rad(angle))
					fci.position.y = fci.position.y + fci.speed * math.cos(math.rad(angle))
					
					fci.point.x = fci.point.x + 2 * math.sin(math.rad(angle))
					fci.point.y = fci.point.y + 2 * math.sin(math.rad(angle))
					
					setFixedCameraPosition(fci.position.x, fci.position.y, fci.position.z, 0.0, 0.0, 0.0)
					pointCameraAtPoint(fci.point.x, fci.point.y, fci.point.z, 0)
				end
				
				if isKeyDown(KEY.VK_S) then
					fci.position.x = fci.position.x - fci.speed * math.sin(math.rad(angle))
					fci.position.y = fci.position.y - fci.speed * math.cos(math.rad(angle))
					
					fci.point.x = fci.point.x - 2 * math.sin(math.rad(angle))
					fci.point.y = fci.point.y - 2 * math.sin(math.rad(angle))
					
					setFixedCameraPosition(fci.position.x, fci.position.y, fci.position.z, 0.0, 0.0, 0.0)
					pointCameraAtPoint(fci.point.x, fci.point.y, fci.point.z, 0)
				end
				
				printStringNow(fci.position.x .. " " .. fci.position.y) 
			end--]]
			
				if targetSet then
					if doesCharExist(targetSet) then
						if isKeyDown(VK_W) then 
							posX = posX + speed 
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						if isKeyDown(VK_S) then 
							posX = posX - speed 
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						if isKeyDown(VK_A) then 
							posY = posY + speed 
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						if isKeyDown(VK_D) then 
							posY = posY - speed 
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						if isKeyDown(VK_SPACE) then  
							posZ = posZ + speed
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						if isKeyDown(VK_SHIFT) then  
							posZ = posZ - speed
							setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
						end
						
						poiX, poiY, poiZ = getCharCoordinates(targetSet)
						pointCameraAtPoint(poiX, poiY, poiZ, 2)
					else
						targetSet = nil
					end
				else
					offMouX, offMouY = getPcMouseMovement()
					  
					offMouX = offMouX / 4.0
					offMouY = offMouY / 4.0
					angZ = angZ + offMouX
					angY = angY + offMouY

					if angZ > 360.0 then angZ = angZ - 360.0 end
					if angZ < 0.0 then angZ = angZ + 360.0 end

					if angY > 89.0 then angY = 89.0 end
					if angY < -89.0 then angY = -89.0 end   

					curZ = angZ + 180.0 
					curY = angY * -1.0      
					radZ = math.rad(curZ) 
					radY = math.rad(curY)                   
					sinZ = math.sin(radZ)
					cosZ = math.cos(radZ)      
					sinY = math.sin(radY)
					cosY = math.cos(radY)       
					sinZ = sinZ * cosY      
					cosZ = cosZ * cosY 
					sinZ = sinZ * 10.0     
					cosZ = cosZ * 10.0       
					sinY = sinY * 10.0                       
					posPlX = posX + sinZ 
					posPlY = posY + cosZ 
					posPlZ = posZ + sinY              
					angPlZ = angZ * -1.0

					if isKeyDown(VK_W) then      
						radZ = math.rad(angZ) 
						radY = math.rad(angY)                   
						sinZ = math.sin(radZ)
						cosZ = math.cos(radZ)      
						sinY = math.sin(radY)
						cosY = math.cos(radY)       
						sinZ = sinZ * cosY      
						cosZ = cosZ * cosY 
						sinZ = sinZ * speed      
						cosZ = cosZ * speed       
						sinY = sinY * speed  
						posX = posX + sinZ 
						posY = posY + cosZ 
						posZ = posZ + sinY      
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)      
					end 

					if isKeyDown(VK_S) then  
						curZ = angZ + 180.0
						curY = angY * -1.0      
						radZ = math.rad(curZ) 
						radY = math.rad(curY)                   
						sinZ = math.sin(radZ)
						cosZ = math.cos(radZ)      
						sinY = math.sin(radY)
						cosY = math.cos(radY)       
						sinZ = sinZ * cosY      
						cosZ = cosZ * cosY 
						sinZ = sinZ * speed      
						cosZ = cosZ * speed       
						sinY = sinY * speed                       
						posX = posX + sinZ 
						posY = posY + cosZ 
						posZ = posZ + sinY      
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
					end 
					  
					if isKeyDown(VK_A) then  
						curZ = angZ - 90.0
						radZ = math.rad(curZ)
						radY = math.rad(angY)
						sinZ = math.sin(radZ)
						cosZ = math.cos(radZ)
						sinZ = sinZ * speed
						cosZ = cosZ * speed
						posX = posX + sinZ
						posY = posY + cosZ
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
					end       

					if isKeyDown(VK_D) then  
						curZ = angZ + 90.0
						radZ = math.rad(curZ)
						radY = math.rad(angY)
						sinZ = math.sin(radZ)
						cosZ = math.cos(radZ)       
						sinZ = sinZ * speed
						cosZ = cosZ * speed
						posX = posX + sinZ
						posY = posY + cosZ      
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
					end 
					
					if isKeyDown(VK_SPACE) then  
						posZ = posZ + speed
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
					end 

					if isKeyDown(VK_SHIFT) then  
						posZ = posZ - speed
						setFixedCameraPosition(posX, posY, posZ, 0.0, 0.0, 0.0)
					end
					
					radZ = math.rad(angZ) 
					radY = math.rad(angY)             
					sinZ = math.sin(radZ)
					cosZ = math.cos(radZ)      
					sinY = math.sin(radY)
					cosY = math.cos(radY)       
					sinZ = sinZ * cosY      
					cosZ = cosZ * cosY 
					sinZ = sinZ * 1.0      
					cosZ = cosZ * 1.0     
					sinY = sinY * 1.0
					if not targetSet then
						poiX = posX
						poiY = posY
						poiZ = posZ   
						poiX = poiX + sinZ 
						poiY = poiY + cosZ 
						poiZ = poiZ + sinY
					end
					
					pointCameraAtPoint(poiX, poiY, poiZ, 2)

					if isKeyDown(187) then 
						speed = speed + 0.0005
					end 
								   
					if isKeyDown(189) then 
						speed = speed - 0.0005
						if speed < 0.001 then speed = 0.001 end
					end 
				end
			end
		end
	end
end

function dynamicTime()
	while true do wait(0)
		if sampIsLocalPlayerSpawned() then
			if set_time then
				setTimeOfDay(set_time[1], set_time[2])
			else
				if ini.set.normalTime then
					local hour, minutes = os.date("%H"), os.date("%M")
					setTimeOfDay(hour, minutes)
				end
			end
		end
	end
end

function notifications()
	while true do wait(0)
		for name, value in pairs(notificationsTimer) do
			if value.switch then
				local hour, minute, second = os.date("%H"), os.date("%M"), os.date("%S")
				if (value.time.hour == tonumber(hour)) and (value.time.minute == tonumber(minute)) and (tonumber(second) == 1) then
					local caption = string.format("{e6e6fa}Уведомление. {00CC66}%s.", value.name)
					local text = string.format("{e6e6fa}%s", value.text)
					if value.sound then addOneOffSound(0.0, 0.0, 0.0, 1058) end
					sampShowDialog(500, caption, text, "Хорошо", "Закрыть", 0)
					value.switch = false
				end
			end
		end
	end
end

function renderPlayerText()
	local patrol_mark, patrol_mark_update = "UNIT", os.clock()
	
	while true do wait(0)
		local step, step_vehicle = 0.1, 0.15
		
		if ini["set"]["patrol"] then
			if global_patrol then
				if os.clock() - patrol_mark_update > 5 then
					patrol_mark = string.upper(sampGetMarkCharByVehicle(playerPed))
					patrol_mark_update = os.clock()
				end
				
				if isCharInAnyCar(playerPed) then
					step_vehicle = step_vehicle - 0.1
					fX, fY, fZ = getOffsetFromCarInWorldCoords(storeCarCharIsInNoSave(playerPed), 1.2, 0.0, step_vehicle)
				else 
					step = step - 0.05
					fX, fY, fZ = getOffsetFromCharInWorldCoords(playerPed, 0.33, 0.0, step)
				end local x, y = convert3DCoordsToScreen(fX, fY, fZ)
				
				local time
				if start_code_1 then
					time = 30 - (os.clock() - start_code_1)
					if time < 0 then start_code_1 = nil end
				end
				
				local text = start_code_1 and string.format("PRESS {00CC66}Y{ffffff} TO DECLARE {C22222}CODE 1{ffffff}! (%d OF 30)", time) or string.format("%s | CODE: {C22222}%d", patrol_mark, global_patrol)
				renderFontDrawText(fontSuspect4, text, x, y, 0xFFFFFFFF)
			end
		end
	
		if maskTimer.state then
			local alltime = 600 - math.floor(os.clock() - maskTimer.clock)
			if isCharInAnyCar(playerPed) then
				step_vehicle = step_vehicle - 0.1
				fX, fY, fZ = getOffsetFromCarInWorldCoords(storeCarCharIsInNoSave(playerPed), 1.2, 0.0, step_vehicle)
			else
				step = step - 0.05
				fX, fY, fZ = getOffsetFromCharInWorldCoords(playerPed, 0.33, 0.0, step)
			end local x, y = convert3DCoordsToScreen(fX, fY, fZ)
			local text = string.format("{C22222}%d{e6e6fa} MINUTES {C22222}%d{e6e6fa} SECONDS", math.floor(alltime / 60), math.fmod(alltime, 60))
			renderFontDrawText(fontSuspect4, text, x, y, 0xFFFFFFFF)
		end
		
		if healmeTimerForPlayer then
			if os.clock() - healmeTimerForPlayer < 5.5 then
				local fX, fY, fZ
				if isCharInAnyCar(playerPed) then
					step_vehicle = step_vehicle - 0.1
					fX, fY, fZ = getOffsetFromCarInWorldCoords(storeCarCharIsInNoSave(playerPed), 1.2, 0.0, step_vehicle)
				else
					step = step - 0.05
					fX, fY, fZ = getOffsetFromCharInWorldCoords(playerPed, 0.33, 0.0, step)
				end local x, y = convert3DCoordsToScreen(fX, fY, fZ)
				local text = string.format("{e6e6fa}HEALME ANIMATION {C22222}%0.3f{e6e6fa} SECONDS", 5.5 - (os.clock() - healmeTimerForPlayer))
				renderFontDrawText(fontSuspect4, text, x, y, 0xFFFFFFFF)
			else healmeTimerForPlayer = nil end
		end
	end
end

function speedometr()
	local function button(font, text, x, y, w, h, bool, c1, c2)
		local c1, c2 = c1 or 0xBBC1CDC1, c2 or 0x77C1CDC1
		renderDrawBox(x, y, w, h, (bool and c1 or c2))
		renderDrawLine(x + 4, y + h, x + w + 2, y + h, 2, 0xDDFFFFFF)
		local fix = renderGetFontDrawTextLength(font, tostring(text))
		renderFontDrawText(font, text, x + ((w) / 2 - fix / 2), y, (bool and 0xffFFFFFF or 0xbbFFFFFF))
	end
	
	local gfe = {
		li = {[0] = "ЛИТРОВ", "ЛИТР", "ЛИТРА", "ЛИТРА", "ЛИТРА", "ЛИТРОВ", "ЛИТРОВ", "ЛИТРОВ", "ЛИТРОВ", "ЛИТРОВ"},
		e = {[0] = "ЕДИНИЦ", "ЕДИНИЦА", "ЕДИНИЦЫ", "ЕДИНИЦЫ", "ЕДИНИЦЫ", "ЕДИНИЦ", "ЕДИНИЦ", "ЕДИНИЦ", "ЕДИНИЦ", "ЕДИНИЦ"}
	}
	
	local function ttfix(number, tab)
		return gfe[tab][math.fmod(number, 10)] or "ОШИБКА"
	end
	
	local x, y = ini.set.ivehiclebarX, ini.set.ivehiclebarY

	while true do
		wait(0)

		if isCharSittingInAnyCar(playerPed) then
			if not speedometr_id or not sampTextdrawIsExists(speedometr_id) then
				for number = 0, 3000 do
					if sampTextdrawIsExists(number) then
						if sampTextdrawGetString(number):find("Fuel") then
							speedometr_id = number
							break
						end
					end
				end
			else
				if sampTextdrawIsExists(speedometr_id) then
					local vehicle = storeCarCharIsInNoSave(playerPed)
					local speedometr_text = speedometr_text or sampTextdrawGetString(speedometr_id)
					
					if not ini.set.ivehiclebar then
						local vehicle_speed = math.ceil(getCarSpeed(vehicle) * 2.02)
						local speedometr_text = sampTextdrawGetString(speedometr_id)
						local speedometr_text = speedometr_text:gsub("%d+_km/h", string.format("%d_km/h", vehicle_speed))
						sampTextdrawSetString(speedometr_id, speedometr_text)
					else
						local v = {}
						v.engine = isCarEngineOn(vehicle)
						v.health = getCarHealth(vehicle)
						v.speed = getCarSpeed(vehicle) * 2.02
						v.fuel = tonumber(string.match(speedometr_text, "Fuel_(%d+)"))
						v.sport = string.find(speedometr_text, "~y~Sport")
						v.light = string.find(speedometr_text, "~g~L")
						v.alorm = not string.find(speedometr_text, "~w~S")
						v.door = getCarDoorLockStatus(vehicle)
						
						--v.fuel, v.light, v.sport, v.alarm = 0, false, false, false
						
						v.lock = (v.door == 2)
						v.tspeed = string.format("%0.2f КМ/Ч", v.speed)
						v.tfuel = v.fuel .. " " .. ttfix(v.fuel, "li")
						v.thealth = v.health .. " " .. ttfix(v.health, "e")
						
						renderDrawBox(ini.set.ivehiclebarX, ini.set.ivehiclebarY + 1, 291, 15, 0x88A52A2A) -- speed
						renderDrawBox(ini.set.ivehiclebarX, ini.set.ivehiclebarY + 20, 144, 10, 0x882F4F4F) -- oil
						renderDrawBox(ini.set.ivehiclebarX + 147, ini.set.ivehiclebarY + 20, 145, 10, 0x88bf2233) -- health
						
						if v.speed > 200 then v.speed = 200 end -- fix speed
						if v.fuel > 150  then v.fuel  = 150 end -- fix speed
						
						local fix1 = renderGetFontDrawTextLength(fontSuspect3, v.tspeed) -- fix pos speed
						renderDrawBox(ini.set.ivehiclebarX + 1, ini.set.ivehiclebarY + 2, v.speed / 2 * 2.91, 14, 0xddA52A2A) -- draw speed bar
						renderFontDrawText(fontSuspect3, v.tspeed, ini.set.ivehiclebarX + (147.5 - fix1 / 2),  ini.set.ivehiclebarY + 1.3, 0xFFFFFFFF) -- draw speed text
						
						if v.sport then
							renderFontDrawText(fontSuspect3, "SPORT", ini.set.ivehiclebarX + 257,  ini.set.ivehiclebarY + 1.3, 0xFFFFFFFF)
						end -- draw sport
						
						local fix2 = renderGetFontDrawTextLength(fontSuspect3, v.tfuel) -- fix pos fuel
						renderDrawBox(ini.set.ivehiclebarX + 1, ini.set.ivehiclebarY + 21, v.fuel / 1.5 * 1.44, 9, 0xdd2F4F4F) -- draw fuel bar
						renderFontDrawText(fontSuspect3, v.tfuel, ini.set.ivehiclebarX + (72 - fix2 / 2),  ini.set.ivehiclebarY + 18, 0xFFFFFFFF) -- draw fuel text
						
						local fix3 = renderGetFontDrawTextLength(fontSuspect3, v.thealth) -- fix pos health
						renderDrawBox(ini.set.ivehiclebarX + 148, ini.set.ivehiclebarY + 21, v.health / 10 * 1.44, 9, 0xddbf2233) -- draw health bar
						renderFontDrawText(fontSuspect3, v.thealth, ini.set.ivehiclebarX + 147 + (72 - fix3 / 2),  ini.set.ivehiclebarY + 18, 0xFFFFFFFF)  -- draw health text
						
						button(fontSuspect2, "LOCK", ini.set.ivehiclebarX, ini.set.ivehiclebarY + 39, 65, 20, v.lock, 0xbbbf2233) -- button lock
						button(fontSuspect2, "ENGINE", ini.set.ivehiclebarX + 74, ini.set.ivehiclebarY + 39, 65, 20, v.engine) -- button engine
						button(fontSuspect2, "LIGHT", ini.set.ivehiclebarX + 148, ini.set.ivehiclebarY + 39, 65, 20, v.light) -- button light
						button(fontSuspect2, "ALORM", ini.set.ivehiclebarX + 222, ini.set.ivehiclebarY + 39, 64, 20, v.alorm) -- button alorm
					end
				end
			end
		end
	end
end

function logotyp()
	local server_name = {"", {"RED", "{CD0000}"}, {"GREEN", "{228B22}"}, {"BLUE", "{4876FF}"}, {"LIME", "{90EE90}"}}
	local serverName

	--local font_logo = renderCreateFont("Tahoma", 13, font_flag.BOLD)
	--local font_logo_lil = renderCreateFont("Tahoma", 11, font_flag.BOLD)
	
	if string.match(sampGetCurrentServerAddress(), "54.37.142.7(%d)") then
		local id = string.match(sampGetCurrentServerAddress(), "54.37.142.7(%d)")
		serverName = server_name[tonumber(id)][1]

		text_logo_lil = "ROLE PLAY " .. serverName

		local fix = renderGetFontDrawTextLength(font_logo_lil, text_logo_lil)
		posLogoX = (ini.set.logo_x + 98.5) - fix / 2
	end

	while true and serverName do
		wait(0)
		
		if destroyLogototyp then 
			if string.match(sampGetCurrentServerAddress(), "54.37.142.7(%d)") then
				local id = string.match(sampGetCurrentServerAddress(), "54.37.142.7(%d)")
				serverName = server_name[tonumber(id)][1]

				text_logo_lil = "ROLE PLAY " .. serverName

				local fix = renderGetFontDrawTextLength(font_logo_lil, text_logo_lil)
				posLogoX = (ini.set.logo_x + 98.5) - fix / 2
			end
			
			destroyLogototyp = false 
		end

		if sampIsLocalPlayerSpawned() then
			if ini.set.new_logo then
				if not draw_new_logo then
					logo_texture = renderLoadTextureFromFileInMemory(memory.strptr(logotype_texture), 4096)
					draw_new_logo = true
				else
					renderFontDrawText(font_logo, "ADVANCE", ini.set.logo_x + 57, ini.set.logo_y + 7, 0xFFFFFFFF)
					renderFontDrawText(font_logo_lil, text_logo_lil, posLogoX, ini.set.logo_y + 25, 0xFFFFFFFF)

					renderDrawTexture(logo_texture, ini.set.logo_x, ini.set.logo_y, 50, 50, 0, 0xFFFFFFFF)
				end
			end
		end
	end
end

function suspectBar()
	local _bsuspect = {name = "", staticName = ""}

	while true do wait(0)
		if ini.set.barSuspect and suspect_id[4] ~= "" then
			if suspect_id[4] ~= _bsuspect.staticName then
				if isPlayerConnected(suspect_id[1]) then
					_bsuspect.staticName = sampGetPlayerName(suspect_id[1])
					_bsuspect.name = string.upper(string.gsub(sampGetPlayerName(suspect_id[1]), "_", " "))
				end
			end
				
			_bsuspect.id = sampGetPlayerIdByNickname(_bsuspect.staticName)
			if _bsuspect.id ~= "OFF" then
				_bsuspect.result, _bsuspect.ped = sampGetCharHandleBySampPlayerId(_bsuspect.id)
				if _bsuspect.result then
					if sampIsPlayerPaused(_bsuspect.id) then
						suspectColor = (math.fmod(math.floor(os.clock()), 2) == 0) and 0xFF7CCD7C or 0xFFB22222
					else suspectColor = 0xFF00CD66 end
				else suspectColor = 0xFFFFFF00 end
			else suspectColor = 0xFFB22222 end
						
				if doesCharExist(_bsuspect.ped) then
					if isCharSittingInAnyCar(_bsuspect.ped) then
						_bsuspect.vehicle = storeCarCharIsInNoSave(_bsuspect.ped)
						_bsuspect.model = getCarModel(_bsuspect.vehicle)
						_bsuspect.vhealth = getCarHealth(_bsuspect.vehicle)
						_bsuspect.nameModel = string.upper(tCarsName[_bsuspect.model - 399]) .. " (" .. _bsuspect.vhealth .. ")"
					else _bsuspect.nameModel = "НЕ В ТРАНСПОРТЕ" end
					_bsuspect.speed = math.ceil(getCharSpeed(_bsuspect.ped) * 2.02)
				else
					_bsuspect.speed = "XXX"
					_bsuspect.nameModel = "НЕИЗВЕСТНО"
				end
				
				local fix = renderGetFontDrawTextLength(fontSuspect1, tostring(_bsuspect.speed))
				
				renderDrawTextShadow(fontSuspect1, _bsuspect.speed, ini.set.barSuspectX + 16.4 - fix / 2, ini.set.barSuspectY)
				renderDrawTextShadow(fontSuspect3, "КМ/Ч", ini.set.barSuspectX + 4, ini.set.barSuspectY + 18)

				renderDrawBox(ini.set.barSuspectX + 40, ini.set.barSuspectY, 3, 30, 0xAAFFFFFF)

				renderDrawTextShadow(fontSuspect2, _bsuspect.name, ini.set.barSuspectX + 53, ini.set.barSuspectY)
				renderDrawTextShadow(fontSuspect3, "•", ini.set.barSuspectX + 54, ini.set.barSuspectY + 15, suspectColor)
				renderDrawTextShadow(fontSuspect3, " " .. _bsuspect.nameModel, ini.set.barSuspectX + 59.5, ini.set.barSuspectY + 15)
				
				if isKeyDown(VK_RBUTTON) then
					if _bsuspect.id ~= "OFF" then
						local line = string.format("ID (%d) LEVEL (%d) PING (%d)", _bsuspect.id, sampGetPlayerScore(_bsuspect.id), sampGetPlayerPing(_bsuspect.id))
						renderDrawTextShadow(fontSuspect3, line, ini.set.barSuspectX + 53, ini.set.barSuspectY + 30, 0xFFFFFFFF)
						renderDrawTextShadow(fontSuspect3, "ДЛЯ СКРЫТИЯ ИНФОРМАЦИИ НАЖМИТЕ {7CCD7C}N", ini.set.barSuspectX + 53, ini.set.barSuspectY + 40, 0xFFFFFFFF)
					else
						renderDrawTextShadow(fontSuspect3, "ДЛЯ СКРЫТИЯ ИНФОРМАЦИИ НАЖМИТЕ {7CCD7C}N", ini.set.barSuspectX + 53, ini.set.barSuspectY + 30, 0xFFFFFFFF)
					end
				
					if wasKeyPressed(KEY.VK_N) then
						suspect_id = {0, 0, "", "", -1}
					end
				end
			--end
		end
	end
end

function command_patrol()
	if global_patrol then global_patrol = nil
	else global_patrol = 4 end
end

function command_phone()
	showPhone = not showPhone
end

function command_menu()
	show_menu.v = not show_menu.v
end

function command_push()
	show_notification.v = not show_notification.v
end

function command_server()
	show_server.v = not show_server.v
end

function command_strobes()
	if isCharInAnyCar(playerPed) and ini.set.strobes  then
		boolStrobes = not boolStrobes
	end
end

function command_distanceChat()
	if player_status > 1 then
		distanceChat = not distanceChat
		chat("Режим прослушивания " .. (distanceChat and "активирован." or "деактивирован."))
	end
end

function command_savepass()
	if savePass and type(savePass) == ("table") then
		local result, id = sampGetPlayerIdByCharHandle(playerPed)

		if result then
			local name = sampGetPlayerName(id)
			local ip = sampGetCurrentServerAddress()
			local dd = string.format("%s_%s", name, ip)

			if savePass.dd == dd then
				if pass.server[dd] == nil then
					local newPass = savePass.pass

					newData = newPass .. " | -1"
				else
					local pass, seckey = pass.server[dd]:match("(.+)[%s]%|[%s](.+)")
					local newPass = savePass.pass

					newData = newPass .. " | " .. seckey
				end

				pass.server[dd] = newData
				inicfg.save(pass, "HfMIA servers and passwords.ini")
				savePass = nil

				chat("Вы успешено сохранили новые данные в менеджере аккаунтов.")
			else
				chat("В данный момент вы не можете обновить данные в менеджере аккаунтов. Ошибка #3.")
			end
		else
			chat("В данный момент вы не можете обновить данные в менеджере аккаунтов. Ошибка #2.")
		end
	else
		chat("В данный момент вы не можете обновить данные в менеджере аккаунтов. Ошибка #1.")
	end
end

function command_r(text)
	if not string.match(text, "(.+)") then chat_error("Введите необходимые параметры для /r [текст].")
	else
		local mark = sampGetMarkCharByVehicle(playerPed)
		
		local tab = {
			["cod 11"] = {"$m to DISP, занимаю маркировку $m, CODE: 1-1, доступен.", function() global_patrol = 4 end},
			["cod 13"] = {"$m to DISP, освобождаю маркировку $m, CODE: 1-3, недоступен.", function() global_patrol = 4 end},
			["cod 14"] = {"$m to DISP, доставляю подозреваемого в департамент, CODE: 1-4, недоступен.", function() global_patrol = 4 end},
			["tf 55"] = {"$m to DISP, провожу 10-55, CODE: 4, 10-20 $p, недоступен.", function() global_patrol = 4 end},
			["tf 66"] = {"$m to DISP, провожу 10-66, CODE: 3, 10-20 $p, недоступен.", function() global_patrol = 3 end},
			["tf 9957v"] = {"$m to DISP, 10-99 по 10-57 V, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 9957f"] = {"$m to DISP, 10-99 по 10-57 F, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 995"] = {"$m to DISP, 10-99 по 10-55, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 996"] = {"$m to DISP, 10-99 по 10-66, CODE: 4, доступен.", function() global_patrol = 4 end},
			["cod 0"] = {"$m to DISP, CODE: 0, требуется срочная помощь! 10-20 $p, недоступен.", function() global_patrol = 0 end},
			["cod 1"] = {"$m to DISP, CODE: 1, требуется помощь! 10-20 $p, недоступен.", function() global_patrol = 1 end},
			["s 99"] = {"$m to DISP, 10-99 по последней ситуации, CODE: 4, доступен.", function() global_patrol = 4 end}
		}
		
		for name, value in pairs(tab) do
			if text == name then
				text = string.gsub(text, name, value[1])
				value[2]()
			end
		end
		
		text = string.gsub(text, "%$m", mark)
		text = string.gsub(text, "%$p", calculateZone())
		text = string.format("%s %s", ini.info.rteg, text)
		
		sampSendChat(string.format("/r %s", text))
		
		if ini.lowrp.rpradio and ini.set.lowrp then
			lua_thread.create(function()
				wait(800)
				sampSendChat(ini.info.male and "/me сняла тангету с плеча, зажала кнопку PTT и что-то произнесла по рации." or "/me снял тангету с плеча, зажал кнопку PTT и что-то произнёс по рации.")
			end)
		end
	end
end

function command_rn(text) 
	if not string.match(text, "(%S+)") then chat_error("Введите необходимые параметры для /rn [текст].") else sampSendChat(string.format("/r (( %s ))", text)) end
end

function command_f(text)
	if not string.match(text, "(%S+)") then 
		chat_error("Введите необходимые параметры для /f [текст].")
	else 
		local mark = sampGetMarkCharByVehicle(playerPed)
		
		local tab = {
			["cod 11"] = {"$m to DISP, занимаю маркировку $m, CODE: 1-1, доступен.", function() global_patrol = 4 end},
			["cod 13"] = {"$m to DISP, освобождаю маркировку $m, CODE: 1-3, недоступен.", function() global_patrol = 4 end},
			["cod 14"] = {"$m to DISP, доставляю подозреваемого в департамент, CODE: 1-4, недоступен.", function() global_patrol = 4 end},
			["tf 55"] = {"$m to DISP, провожу 10-55, CODE: 4, 10-20 $p, недоступен.", function() global_patrol = 4 end},
			["tf 66"] = {"$m to DISP, провожу 10-66, CODE: 3, 10-20 $p, недоступен.", function() global_patrol = 3 end},
			["tf 9957v"] = {"$m to DISP, 10-99 по 10-57 V, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 9957f"] = {"$m to DISP, 10-99 по 10-57 F, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 995"] = {"$m to DISP, 10-99 по 10-55, CODE: 4, доступен.", function() global_patrol = 4 end},
			["tf 996"] = {"$m to DISP, 10-99 по 10-66, CODE: 4, доступен.", function() global_patrol = 4 end},
			["cod 0"] = {"$m to DISP, CODE: 0, требуется срочная помощь! 10-20 $p, недоступен.", function() global_patrol = 0 end},
			["cod 1"] = {"$m to DISP, CODE: 1, требуется помощь! 10-20 $p, недоступен.", function() global_patrol = 1 end},
			["s 99"] = {"$m to DISP, 10-99 по последней ситуации, CODE: 4, доступен.", function() global_patrol = 4 end}
		}
		
		for name, value in pairs(tab) do
			if text == name then
				text = string.gsub(text, name, value[1])
				value[2]()
			end
		end
		
		text = string.gsub(text, "%$m", mark)
		text = string.gsub(text, "%$p", calculateZone())
		text = string.format("%s %s", ini.info.fteg, text)
		
		sampSendChat(string.format("/f %s", text))
		
		if ini.lowrp.rpradio then
		
			lua_thread.create(function()
				wait(800)
				sampSendChat(ini.info.male and "/me сняла тангету с плеча, зажала кнопку PTT и что-то произнесла по рации." or "/me снял тангету с плеча, зажал кнопку PTT и что-то произнёс по рации.")
			end)
		end
	end
end

function command_fn(text)
	if not string.match(text, "(%S+)") then chat_error("Введите необходимые параметры для /fn [текст].") else sampSendChat(string.format("/f (( %s ))", text)) end
end

function command_radiotag()
	local tab = {
		["cod 11"] = {"$m to DISP, занимаю маркировку $m, CODE: 1-1, доступен.", function() global_patrol = 4 end},
		["cod 13"] = {"$m to DISP, освобождаю маркировку $m, CODE: 1-3, недоступен.", function() global_patrol = 4 end},
		["cod 14"] = {"$m to DISP, доставляю подозреваемого в департамент, CODE: 1-4, недоступен.", function() global_patrol = 4 end},
		["tf 55"] = {"$m to DISP, провожу 10-55, CODE: 4, 10-20 $p, недоступен.", function() global_patrol = 4 end},
		["tf 66"] = {"$m to DISP, провожу 10-66, CODE: 3, 10-20 $p, недоступен.", function() global_patrol = 3 end},
		["tf 9957v"] = {"$m to DISP, 10-99 по 10-57 V, CODE: 4, доступен.", function() global_patrol = 4 end},
		["tf 9957f"] = {"$m to DISP, 10-99 по 10-57 F, CODE: 4, доступен.", function() global_patrol = 4 end},
		["tf 995"] = {"$m to DISP, 10-99 по 10-55, CODE: 4, доступен.", function() global_patrol = 4 end},
		["tf 996"] = {"$m to DISP, 10-99 по 10-66, CODE: 4, доступен.", function() global_patrol = 4 end},
		["cod 0"] = {"$m to DISP, CODE: 0, требуется срочная помощь! 10-20 $p, недоступен.", function() global_patrol = 0 end},
		["cod 1"] = {"$m to DISP, CODE: 1, требуется помощь! 10-20 $p, недоступен.", function() global_patrol = 1 end},
		["s 99"] = {"$m to DISP, 10-99 по последней ситуации, CODE: 4, доступен.", function() global_patrol = 4 end}
	}
	
	local text = "{e6e6fa}Код\t\t{00CC66}Маркировка{e6e6fa}, {FFCD00}местоположение{e6e6fa} и содержание."
	for index, value in pairs(tab) do
		text = string.format("%s\n{FFCD00}%s{e6e6fa}\t\t%s", text, index, value[1])
	end
	
	local text = string.gsub(text, "%$m", "{00CC66}" .. sampGetMarkCharByVehicle(playerPed) .. "{e6e6fa}")
	local text = string.gsub(text, "%$p", string.format("{FFCD00}%s (%s){e6e6fa}", calculateZone(), kvadrat()))

	sampShowDialog(1005, "{FFCD00}Список радио-тэгов для рации", text, "Закрыть", "", 5)
end

function command_id(parametrs)
	if string.match(parametrs, "(%S+)") then
		if tonumber(parametrs) then
			local id = tonumber(parametrs)
			local name = sampGetPlayerNickname(id)
			
			if ini.number[name] then
				chat(string.format("{%s}%s{} id {00CC66}%d{}, телефонный номер {00CC66}%d{}.", sampGetColorByPlayer(id), name, id, ini.number[name]))
			else
				chat(string.format("{%s}%s{} id {00CC66}%d{}.", sampGetColorByPlayer(id), name, id))
			end
		else
			local found = {0, {}}
			for id = 0, 1000 do
				if isPlayerConnected(id) then
					local name = sampGetPlayerNickname(id)
					if string.match(string.upper(name), string.upper(parametrs)) then
						found[1] = found[1] + 1
						table.insert(found[2], {id, name})
					end
				end
			end
			
			if found[1] > 0 then
				if found[1] > 5 then
					for i = 1, 5 do
						local name, id = found[2][i][2], found[2][i][1]
						if ini.number[name] then
							chat(string.format("%d. {%s}%s{} id {00CC66}%d{}, телефонный номер {00CC66}%d{}.", i, sampGetColorByPlayer(id), name, id, ini.number[name]))
						else
							chat(string.format("%d. {%s}%s{} id {00CC66}%d{}.", i, sampGetColorByPlayer(id), name, id))
						end
					end
					chat(string.format("Показаны первые {00CC66}5{} из {00CC66}%d{} совпадений.", found[1]))
				else
					for index, name in ipairs(found[2]) do
						local _name, id = name[2], name[1]
						if ini.number[_name] then
							chat(string.format("%d. {%s}%s{} id {00CC66}%d{}, телефонный номер {00CC66}%d{}.", index, sampGetColorByPlayer(id), _name, id, ini.number[_name]))
						else
							chat(string.format("%d. {%s}%s{} id {00CC66}%d{}.", index, sampGetColorByPlayer(id), _name, id))
						end
					end
				end
			else
				chat("Игроков с подобным никнейном или частью никнейма не найдено.")
			end
		end
	else
		chat_error("Введите необходимые параметры для /id [ид игрока / никнейм или часть никнейма].")
	end
end

function command_suspect(id, bool)
	if bool then
		if tonumber(id) then
			wantedAssistant = {id, false}
			sampSendChat("/wanted")
		end
	else
		if tonumber(id) then
			if isPlayerConnected(id) then
				wantedAssistant = {id, true}
				sampSendChat("/wanted")
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID или введите никнейм.")
			end
		else
			chat_error("Введите необходимые параметры для /suspect [ид игрока].")
		end
	end
end

function command_dropall()
	dropAll = true
	sampSendChat("/drop")
end

function command_reconnect(parametrs)
	if globalReconnectStatus then
		chat("Невозможно переподключится к другому серверу пока идёт переподключение.")
		return
	end
	
	if string.match(parametrs, "(%d+)") then
		local delay = tonumber(parametrs)
		if delay > 0 and delay <= 60 then
			reconnect(delay)
		else
			chat("Задержка между переподключениями не должна быть менее 0 и более 60 секунд.")
		end
	else
		chat_error("Введите необходимые параметры для /rec [задержка].")
	end
end

function command_reconnectwithchange(parametrs)
	if globalReconnectStatus then
		chat("Невозможно переподключится к другому серверу пока идёт переподключение.")
		return
	end

	if string.match(parametrs, "(%d+) (%S+)") then
		local delay, name = string.match(parametrs, "(%d+) (.+)")
		local delay = tonumber(delay)
		if delay > 0 and delay <= 60 then
			sampSetLocalPlayerName(name)
			reconnect(delay)
		else
			chat("Задержка между переподключениями не должна быть менее 0 и более 60 секунд.")
		end
	else
		chat_error("Введите необходимые параметры для /recn [задержка] [никнейм].")
	end
end

function command_reconnectwithchangeip(parametrs)
	if globalReconnectStatus then
		chat("Невозможно переподключится к другому серверу пока идёт переподключение.")
		return
	end

	if string.match(parametrs, "(%S+) (%S+)") then
		local ip, name = string.match(parametrs, "(%S+) (%S+)")
		if string.match(ip, "(%S+):(%d+)") then
			sampSetLocalPlayerName(name)
			reconnect(1, ip)
		else
			chat("IP-адресс сервера должен быть в формате {00CC66}IP:PORT{} (например: {00CC66}5.254.104.132:7777{}).")
		end
	elseif string.match(parametrs, "(%S+)") then
		local ip = parametrs
		if string.match(ip, "(%S+):(%d+)") then
			sampSetLocalPlayerName(name)
			reconnect(1, ip)
		else
			chat("IP-адресс сервера должен быть в формате {00CC66}IP:PORT{} (например: {00CC66}5.254.104.132:7777{}).")
		end
	else
		chat_error("Введите необходимые параметры для /recd [IP-адресс сервера] [никнейм (необязательно)].")
	end
end

function reconnect(delay, ip)
	if type(delay) == ("number") and ip == nil then
		lua_thread.create(function()
			local wdelay = delay * 1000
			sampDisconnectWithReason(1)
			local startTime = os.clock()
			local x, y = getScreenResolution()
			local x, y = x / 2, y / 2
			globalReconnectStatus = true
				
			while os.clock() - startTime < delay do wait(0)
				local text = string.format("{ffffff}ПЕРЕДПОКЛЮЧЕНИЕ ПРОИЗОЙДЁТ ЧЕРЕЗ {00CC66}%0.3f{ffffff} СЕК.", delay - (os.clock() - startTime))
				local fix = renderGetFontDrawTextLength(fontSuspect2, text)
				local _fix = fix / 2
				renderDrawBox(x - _fix - 5, y - 5, fix + 10, 30, 0xAA363636)
				renderDrawLine(x - _fix - 4, y + 25, x + _fix + 5, y + 25, 2, 0xFFFFFFFF)
				renderFontDrawText(fontSuspect2, text, x - _fix, y, 0xFFFFFFFF)
			end
			
			globalReconnectStatus = false
			sampSetGamestate(1)
		end)
	elseif type(delay) == ("number") and type(ip) == ("string") then
		lua_thread.create(function()
			if string.match(ip, "(%S+):(%d+)") then
				local ipadress, port = string.match(ip, "(.+):(%d+)")
				local port = tonumber(port)
				local wdelay = delay * 1000  
				local startTime = os.clock()
				local x, y = getScreenResolution()
				local x, y = x / 2, y / 2
				globalReconnectStatus = true
				
				while os.clock() - startTime < delay do wait(0)
					local text = string.format("{ffffff}ПЕРЕДПОКЛЮЧЕНИЕ ПРОИЗОЙДЁТ ЧЕРЕЗ {00CC66}%0.3f{ffffff} СЕК.", delay - (os.clock() - startTime))
					local fix = renderGetFontDrawTextLength(fontSuspect2, text)
					local _fix = fix / 2
					renderDrawBox(x - _fix - 5, y - 5, fix + 10, 30, 0xAA363636)
					renderDrawLine(x - _fix - 4, y + 25, x + _fix + 5, y + 25, 2, 0xFFFFFFFF)
					renderFontDrawText(fontSuspect2, text, x - _fix, y, 0xFFFFFFFF)
				end
				
				globalReconnectStatus = false
				sampConnectToServer(ipadress, port)
				destroyLogototyp = true
			end
		end)
	end
end

function command_rkinfo()
	if dreason and type(dreason) == ("table") then
		local alltime = ini.set.delay_rk * 60 - math.floor(os.clock() - dreason[2])

		local minute = math.floor(alltime / 60)
		local second = math.fmod(alltime, 60)

		if alltime > 0 and alltime < ini.set.delay_rk * 60 then
			if tostring(calculateZone()) == tostring(dreason[1]) then
				chat(string.format("[{ff5c33}WARN{}] Вы можете вернуться в район {ff5c33}%s{} через %d мин, %d сек ({00CC66}%d{} сек).", dreason[1], minute, second, alltime))
			else
				chat(string.format("Вы можете вернуться в район {ff5c33}%s{} через %d мин, %d сек ({00CC66}%d{} сек).", dreason[1], minute, second, alltime))
			end
		else
			chat(string.format("Время вышло и вы {00cc99}можете{} вернуться в район %s.", dreason[1]))
		end
	else
		chat("Информация о Вашем RK будет доступна после того, как ваш персонаж погибнет.")
	end
end

--[[function command_rkinfo()
	if dreason[1] then
		local timed = getTimeByPeriod(tonumber(dreason[2]), tonumber(dreason[3]), tonumber(dreason[4]), ini.set.delay_rk)
		chat(string.format("Вы можете вернуться в район %s в %d ч. %d мин. %d сек.", dreason[1], timed[1], timed[2], timed[3]))
		if sampGetPassedTimeByPeriod(tonumber(dreason[2]), tonumber(dreason[3]), tonumber(dreason[4]), ini.set.delay_rk) then
			chat(string.format("Время вышло и вы уже {00cc99}можете{} вернуться в район %s.", dreason[1]))
		else
			chat(string.format("В данный момент Вы {ff5c33}не можете{} вернуться в район %s.", dreason[1]))
			if tostring(calculateZone()) == tostring(dreason[1]) then
				chat(string.format(" {ff5c33}[ПРЕДУПРЕЖДЕНИЕ] {}Вы находитесь в запрещённом для Вас районе %s.", dreason[1]))
			end
		end
	else
		chat("Вы не умирали.")
	end
end]]

function command_apay(param)
	if string.match(param, "(%d+) (%d+)") then
		local id, money = string.match(param, "(%d+) (%d+)")
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 3 then
				if tonumber(money) < 200000 then
					lua_thread.create(function()
						while sampGetDistanceToPlayer(id) < 3 do
							if (tonumber(money) - 5000) <= 0 then
								sampSendChat(string.format("/pay %d %d", id, money))
								break
							else
								sampSendChat(string.format("/pay %d 5000", id, money))
								money = tonumber(money) - 5000
							end
							wait(3500)
						end
					end)
				else
					chat("Невозможно передать более 200.000$.")
				end
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /pay [ид игрока] [сумма].")
	end
end

function table_to_text(tab)
	for i, val in pairs(tab) do
		imgui.TextColoredRGB(val)
	end
end

function command_report(text)
	if not string.match(text, "(%S+)") then
		chat_error("Введите необходимые параметры для /rep [текст].")
	else
		toReport = text
		sampSendChat("/mn")
	end
end

function command_sms(value)
	if string.match(value, "(%d+) (%S+)") then 
		local number, text = string.match(value, "(%d+) (.+)")
		local found = false
		
		if isPlayerConnected(number) then
			local name = sampGetPlayerName(number)
			if ini.number[name] then
				number = ini.number[name]
			else
				chat("Номер игрока не найден в базе данных.")
				found = true
			end
		end
		
		if not found then
			if string.len(text) > 60 then
				sampSendChat(string.format("/sms %d %s ..", number, string.sub(text, 0, 60)))
				sampSendChat(string.format("/sms %d .. %s", number, string.sub(text, 61, string.len(text))))
			else
				sampSendChat(string.format("/sms %d %s", number, string.sub(text, 0, 60)))
			end
		end
	else
		chat_error("Введите необходимые параметры для /sms [номер телефона или ид игрока] [сообщение].")
	end
end

function command_call(parametrs)
	if string.match(parametrs, "(%d+) 1") then
		local id = string.match(parametrs, "(%d+) 1")
		if isPlayerConnected(id) then
			local name = sampGetPlayerName(id)
			if ini.number[name] then
				local number = ini.number[name]
				sampSendChat(string.format("/c %d", number))
			else
				chat("Номер игрока не найден в базе данных.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	elseif tonumber(parametrs) then
		sampSendChat(string.format("/c %d", parametrs))
	else
		sampSendChat("/c")
	end
end

function command_addbl(name)
	if string.match(name, "(%S+)") then
		local path = "moonloader//helper//black-list.txt"
		
		local filew = io.open(path, "a")
		filew:write(name)
		filew:close()
		
		chat(string.format("Игрок {00CC66}%s{} был добавлен в чёрный список.", name))
	else
		chat_error("Введите необходимые параметры для /addbl [имя игрока].")
	end
end

function command_delbl(name)
	if string.match(name, "(%S+)") then
		local path = "moonloader//helper//black-list.txt"
		
		local filer = io.open(path, "r")
		local bl = filer:read("*a")
		filer:close()
		
		if bl:find(name) then
			local text = string.gsub(bl, name, "")
			local filew = io.open(path, "w")
			filew:write(text)
			filew:close()
			
			chat(string.format("Игрок {00CC66}%s{} был удалён из чёрного списка.", name))
		else
			chat("Данного игрока нет в чёрном списке.")
		end
	else
		chat_error("Введите необходимые параметры для /debl [имя игрока].")
	end
end

function command_asms(text)
	if string.match(text, "(%S+)") then
		if last_sms ~= 0 then
			command_sms(string.format("%d %s", last_sms, text))
		else
			chat("Ранее вам никто не отправлял SMS-сообщения.")
		end
	else
		chat_error("Введите необходимые параметры для /lsms [текст].")
	end
end

function command_gov(id) 
	if player_status > 0 then
		if string.match(id, "(%d+)") then
			if gnews[1] then
				local timed = getTimeByPeriod(tonumber(gnews[1]), tonumber(gnews[2]), tonumber(gnews[3]), 5)
				if sampGetPassedTimeByPeriod(tonumber(gnews[1]), tonumber(gnews[2]), tonumber(gnews[3]), 5) then
					send_gnews(id)
				else
					chat(string.format("В данный момент Вы {ff5c33}не можете{} отправить гос. новость.", gnews[1]))
				end
			else
				send_gnews(id)
			end
		else
			if gnews[1] then
				local timed = getTimeByPeriod(tonumber(gnews[1]), tonumber(gnews[2]), tonumber(gnews[3]), 5)
				if sampGetPassedTimeByPeriod(tonumber(gnews[1]), tonumber(gnews[2]), tonumber(gnews[3]), 5) then
					chat_error("Введите необходимые параметры для /gov [ид новости]. Вы {00cc99}можете{} отправить гос. новость.")
				else
					chat_error("Введите необходимые параметры для /gov [ид новости]. Вы {ff5c33}не можете{} отправить гос. новость.")
					chat(string.format("Вы сможете отправить гос. новость в %d ч. %d мин. %d сек.", timed[1], timed[2], timed[3]))
				end
			else
				chat_error("Введите необходимые параметры для /gov [ид новости]. Вы {00cc99}можете{} отправить гос. новость.")
			end
		end
	else
		chat("Вы не можете использовать данную функцию, вы не лидер.")
	end
end

function send_gnews(id)
	if doesFileExist(string.format("moonloader//helper//gnews//news-%d.txt", id)) then
		local gtype = "0"
		lua_thread.create(function()
			for val in io.lines(string.format("moonloader//helper//gnews//news-%d.txt", id)) do
				if string.match(val, "type=(%d+)") then
					gtype = string.match(val, "type=(%d+)")
				elseif string.match(val, "line(%d+)=(.+)") then
					local line, text = string.match(val, "line(%d+)=(.+)")
					if tonumber(gtype) == 3 then
						if tonumber(line) <= 3 then
							if tonumber(line) == 1 then sampSendChat(string.format("/gnews %s", text)) end
							if tonumber(line) == 2 then sampSendChat(string.format("/gnews %s", text)) end
							if tonumber(line) == 3 then sampSendChat(string.format("/gnews %s", text)) end
							gnews = {os.date("%H"), os.date("%M"), os.date("%S"), 3}
						end
					elseif tonumber(gtype) == 1 then
						if tonumber(line) == 4 then sampSendChat(string.format("/gnews %s", text)) end
						gnews = {os.date("%H"), os.date("%M"), os.date("%S"), 1}
					end
					
					wait(1000)
				end
			end
		end)
	else
		chat("Не найдена новость с данным ID.")
	end
end

function decode()
	local f = io.open(thisScript().path, "r")
	local text = f:read("*a")
	f:close()
		
	local fw = io.open(thisScript().path, "w")
	fw:write(u8:decode(text))
	fw:close()
end

function command_rights()
	lua_thread.create(function()
		sampSendChat("Вы имеете право хранить молчание. ")
		wait(1500)
		sampSendChat("Всё, что вы скажете, может и будет использовано против вас в суде. ")
		wait(1500)
		sampSendChat("Ваш адвокат может присутствовать при допросе. ")
		wait(1500)
		sampSendChat("Если вы не можете оплатить услуги адвоката, он будет предоставлен вам государством.")
		wait(1500)
		sampSendChat("Вы понимаете свои права?")
	end)
end

function command_setskin(param)
	if player_status > 1 then
		if string.match(param, "(%d+) (%d+)") then
			local id, skin, state = string.match(param, "(%d+) (%d+)")
			if isPlayerConnected(id) then
				if sampGetDistanceToPlayer(id) < 200 then
					local BS = raknetNewBitStream()
					raknetBitStreamWriteInt32(BS, id)
					raknetBitStreamWriteInt32(BS, skin)
					raknetEmulRpcReceiveBitStream(153, BS)
					raknetDeleteBitStream(BS)
					
					local name = string.gsub(sampGetPlayerName(id),"_"," ")
					chat("Вы установили визуальный скин ({00CC66}" .. skin .. "{}) для игрока " .. name .. "[" .. id .. "].")
				else
					chat("Данный игрок находится слишком далеко от Вас.")
				end
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
			end
		elseif string.match(param, "(%d+)") then
			local skin = string.match(param, "(%d+)")
			local result, id = sampGetPlayerIdByCharHandle(playerPed)

			if result then
				local BS = raknetNewBitStream()
				raknetBitStreamWriteInt32(BS, id)
				raknetBitStreamWriteInt32(BS, skin)
				raknetEmulRpcReceiveBitStream(153, BS)
				raknetDeleteBitStream(BS)
				chat("На Вашего персонажа был установлен визуальный скин {00CC66}" .. skin .. "{}.")
			else
				chat("Произошла ошибка при попытке выдачи скина.")
			end
		else
			chat_error("Введите необходимые параметры для /sskin [ид игрока (необязательно)] [ид скина].")
		end
	end
end

function command_history(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			sampSendChat(string.format("/history %s", sampGetPlayerName(id)))
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID или введите никнейм.")
		end
	elseif string.match(id, "(%S+)") then
		sampSendChat(string.format("/history %s", id))
	else
		chat_error("Введите необходимые параметры для /history [никнейм или ид игрока].")
	end
end

function command_setweather(id)
	if string.match(id, "(%d+)") then 
		local id = string.match(id, "(%d+)")
		if tonumber(id) > 0 and tonumber(id) <= 45 then
			forceWeatherNow(id)
			chat("Вы изменили игровую погоду на {00CC66}" .. id .. "{} ID.")
		else
			chat("ID погоды не должен быть больше 45 и меньше 1.")
		end
	else
		chat_error("Введите необходимые параметры для /sw [ид погоды].")
	end
end

function command_settime(param)
	if string.match(param, "(%d+) (%d+)") then
		local hour, min = string.match(param, "(%d+) (%d+)")
		if tonumber(hour) >= 0 and tonumber(hour) <= 23 and tonumber(min) >= 0 and tonumber(min) < 60 then
			patch_samp_time_set(true)
			set_time = {hour, min}
			chat("Вы изменили игровое время на {00CC66}" .. hour .. "{} часов, {00CC66}" .. min .. "{} минут.")
		else
			patch_samp_time_set(false)
			set_time = nil
			chat("Часы не должны быть больше 23 и меньше 0. Минуты не должны быть больше 59 и меньше 0.")
		end
	else
		if set_time and ini.set.normalTime then
			set_time = nil
			patch_samp_time_set(false)
			chat("Вы перешли к режиму {00CC66}динамического обновления времени{}.")
		else
			chat_error("Введите необходимые параметры для /st [часы] [минуты].")
		end
	end
end

function command_ud()
	lua_thread.create(function()
		if not ini.info.male then
			sampSendChat("/me достал удостоверение, открыл его и показал человеку напротив.")
			wait(1500); sampSendChat("/do В удостоверение: " .. ini.info.frac .. ", " .. ini.info.rang .. ", " .. ini.info.name .. ".")
			wait(1000); sampSendChat("/me закрыл удостоверение и убрал его обратно.")

		else
			sampSendChat("/me достала удостоверение, открыла его и показала человеку напротив.")
			wait(1500); sampSendChat("/do В удостоверение: " .. ini.info.frac .. ", " .. ini.info.rang .. ", " .. ini.info.name .. ".")
			wait(1000); sampSendChat("/me закрыла удостоверение и убрала его обратно.")
		end
	end)
end

function command_pas(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 5 then
				lua_thread.create(function()
					sampSendChat(sampGetHelloByTime() .. ", я " .. ini.info.rang .. " " .. ini.info.name .. ".")
					wait(1000) sampSendChat("/do На груди висит значок с личным номером №000" .. ini.info.number .. ".")
					wait(1500) sampSendChat("Будьте любезны, предъявите документы, удостоверяющие вашу личность.")
					if ini.set.autopass then autopass = true end
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /pas [ид игрока].")
	end
end

function command_infrared()
	if not _nightvision then
		_infrared = not _infrared
		setInfraredVision(_infrared)
		chat("Вы изменили состояние отображения эффекта {00CC66}тепловизора{}.")
	else
		chat("Отключите отображение эффекта {00CC66}прибора ночного видения{}.")
	end
end

function command_nightvision()
	if not _infrared then
		_nightvision = not _nightvision
		setNightVision(_nightvision)
		chat("Вы изменили состояние отображения эффекта {00CC66}прибора ночного виденья{}.")
	else
		chat("Отключите отображение эффекта {00CC66}тепловизора{}.")
	end
end

function command_search(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 5 then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me из внутреннего кармана достал белые, латекстные перчатки и надел их на руки.")
					else
						sampSendChat("/me из внутреннего кармана достала белые, латекстные перчатки и надела их на руки.")
					end
					wait(1500); sampSendChat("/me осмотривает все карманы, возможные места хранение запрещённых веществ и предметов.")
					wait(1000); sampSendChat(string.format("/search %d", id))
					idsearch = id
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /search [ид игрока].")
	end
end

function search_file(file, text, bool)
	if type(file) == ("string") and type(text) == ("string") then
		if doesFileExist(string.format("moonloader\\helper\\%s.txt", file)) then
			for line in io.lines(string.format("moonloader\\helper\\%s.txt", file)) do
				if line:find(text) then 
					for i = 0, string.len(line) / 115 do
						chat(string.sub(line, i * 115, i * 115 + 114))
					end
					
					if not bool then break end
				end
			end
		end
	end
end

function search_law(table, text)
	if table and text then
		for _, val in pairs(table) do
			for i, item in ipairs(val.st) do
				if string.match(item, "(.+) | (.+) | (.+)") then
					local stars, reason, info = string.match(item, "(.+) | (.+) | (.+)")

					if info:find(text) then
						chat(string.format("%s,  %d | %s", reason, stars, info))
						break
					end
				end
			end
		end
	end
end

function command_ustav(text)
	if not string.match(text, "(.+)") then chat_error("Введите необходимые параметры для /ustav [часть или номер статьи]")
	else
		search_file("ustav", text)
	end
end

function command_uk(text)
	if not string.match(text, "(%S+)") then show_uk_list.v = not show_uk_list.v
	else
		--search_law(smart_suspect.text, text)
	end
end

function command_ak(text)
	if not string.match(text, "(%S+)") then show_ak_list.v = not show_ak_list.v
	else
		--search_file("ak", text)
	end
end

function command_code(text)
	if not string.match(text, "(.+)") then chat_error("Введите необходимые параметры для /code [часть или номер кода]")
	else
		search_file("code", text)
	end
end

function command_hack(id)
	if string.match(id, "(%d+)") then
		lua_thread.create(function()
			if not ini.info.male then
				sampSendChat("/do На плечах висит рюкзак в котором лежит балончик с заморозкой.")
				wait(1500); sampSendChat("/me скинув рюкзак с плеч, открыл его и достал балончик.")
				wait(1500); sampSendChat("/me закрыл рюкзак и повесил его на плечи.")
				wait(1500); sampSendChat("/me встряхнув балончик, распылил содержимое на дверной замок.")
				wait(1500); sampSendChat("/do Под действием содержимого балончика замок промёрз и стал хрупок.")
				wait(1500); sampSendChat("/me снял дубинку с поясного держателя и, размахнувшись, ударил тыльной частью по замку.")
			else
				sampSendChat("/do На плечах висит рюкзак в котором лежит балончик с заморозкой.")
				wait(1500); sampSendChat("/me скинув рюкзак с плеч, открыла его и достала балончик.")
				wait(1500); sampSendChat("/me закрыла рюкзак и повесила его на плечи.")
				wait(1500); sampSendChat("/me встряхнув балончик, распылила содержимое на дверной замок.")
				wait(1500); sampSendChat("/do Под действием содержимого балончика замок промёрз и стал хрупок.")
				wait(1500); sampSendChat("/me сняла дубинку с поясного держателя и, размахнувшись, ударила тыльной частью по замку.")
			end
			wait(1000); sampSendChat(string.format("/hack %d", id))
		end)
	else
		chat_error("Введите необходимые параметры для /hack [ид дома].")
	end
end

function command_invite(param)
	if player_status > 0 then
		if string.match(param, "(%d+) (%d+)") then
			inv_id, inv_rang = string.match(param, "(%d+) (%d+)")
			if isPlayerConnected(id) then
				if sampGetDistanceToPlayer(inv_id) < 3 then
					lua_thread.create(function()
						if not ini.info.male then
							sampSendChat("/me из внутреннего кармана достал планшет и включил его.")
							wait(1500); sampSendChat("/me зайдя в базу данных, добавил новое личное дело " .. string.gsub(sampGetPlayerName(inv_id),"_"," ") .. ".")
							wait(1500); sampSendChat("/me потушил экран планшета и убрал его обратно.")
							wait(1500); sampSendChat("/me достал ключ от шкафчика под номером " .. inv_id .. " и передал его " .. string.gsub(sampGetPlayerName(inv_id),"_"," ") .. ".")
						else
							sampSendChat("/me из внутреннего кармана достала планшет и включила его.")
							wait(1500); sampSendChat("/me зайдя в базу данных, добавила новое личное дело " .. string.gsub(sampGetPlayerName(inv_id),"_"," ") .. ".")
							wait(1500); sampSendChat("/me потушила экран планшета и убрала его обратно.")
							wait(1500); sampSendChat("/me достала ключ от шкафчика под номером " .. inv_id .. " и передала его " .. string.gsub(sampGetPlayerName(inv_id),"_"," ") .. ".")
						end
						wait(1000); sampSendChat(string.format("/invite %d", inv_id))
					end)
				else
					chat("Данный игрок находится слишком далеко от Вас.")
				end
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
			end
		else
			chat_error("Введите необходимые параметры для /invite [ид игрока] [ранг].")
		end
	else
		chat("Вы не можете использовать данную функцию, вы не лидер и не его заместитель.")
	end
end

function command_uninvite(param)
	if player_status > 0 then
		if string.match(param, "(%d+) (%S+)") then
			local id, reason = string.match(param, "(%d+) (.+)")
			if isPlayerConnected(id) then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me из внутреннего кармана достал планшет и включил его.")
						wait(1500); sampSendChat("/me зайдя в базу данных, удалил личное дело " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
						wait(1000); sampSendChat(string.format("/uninvite %d %s", id, reason))
						wait(1500); sampSendChat("/me потушил экран планшета и убрал его обратно.")
					else
						sampSendChat("/me из внутреннего кармана достала планшет и включила его.")
						wait(1500); sampSendChat("/me зайдя в базу данных, удалила личное дело " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
						wait(1000); sampSendChat(string.format("/uninvite %d %s", id, reason))
						wait(1500); sampSendChat("/me потушила экран планшета и убрала его обратно.")
					end
					wait(1000); command_r("Сотрудник " .. string.gsub(sampGetPlayerName(id),"_"," ") .. " уволен по причине: " .. reason)
				end)
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
			end
		else
			chat_error("Введите необходимые параметры для /uninvite [ид игрока] [причина].")
		end
	else
		chat("Вы не можете использовать данную функцию, вы не лидер и не его заместитель.")
	end
end

function command_follow()
	lua_thread.create(function()
		if follow_state then
			if not ini.info.male then
				sampSendChat("/me достал планшет, перешёл в базу данных и выбрал пункт с настройкой радио-канала.")
				wait(1000); sampSendChat("/follow")
				wait(1500); sampSendChat("/me отключился от внутреннего радио-канала, снял наушники и убрал их.")
				wait(1500); sampSendChat("/me потушил экран планшета, убрал его обратно.")
			else
				sampSendChat("/me достала планшет, перешла в базу данных и выбрала пункт с настройкой радио-канала.")
				wait(1000); sampSendChat("/follow")
				wait(1500); sampSendChat("/me отключилась от внутреннего радио-канала, сняла наушники и убрала их.")
				wait(1500); sampSendChat("/me потушила экран планшета, убрала его обратно.")
			end
		else
			if not ini.info.male then
				sampSendChat("/me достал планшет, перешёл в базу данных и выбрал пункт с настройкой радио-канала.")
				wait(1500); sampSendChat("/me ввёл необходимые параметры для подключенния к внутреннему радио-каналу.")
				wait(1500); sampSendChat("/me достал наушники, параллельно заполнив нужную форму получения радио-сигналов.")
				wait(1000); sampSendChat("/follow")
				wait(1500); sampSendChat("/me надел наушники, проверил звук, потушил экран планшета и убрал его.")
			else
				sampSendChat("/me достала планшет, перешла в базу данных и выбрала пункт с настройкой радио-канала.")
				wait(1500); sampSendChat("/me ввела необходимые параметры для подключенния к внутреннему радио-каналу.")
				wait(1500); sampSendChat("/me достала наушники, параллельно заполнив нужную форму получения радио-сигналов.")
				wait(1000); sampSendChat("/follow")
				wait(1500); sampSendChat("/me надела наушники, проверила звук, потушила экран планшета и убрала его.")
			end
		end
	end)
end

function command_rang(param)
	if player_status > 0 then
		if string.match(param, "(%d+) [+?-]") then
			local id, rang = string.match(param, "(%d+) (.+)")
			if isPlayerConnected(id) then
				if sampGetDistanceToPlayer(id) < 3 then
					lua_thread.create(function()
						if not ini.info.male then
							sampSendChat("/me из внутреннего кармана достал планшет и включил его.")
							wait(1500); sampSendChat("/me зайдя в базу данных, изменил должность " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
							wait(1000); sampSendChat(string.format("/rang %d %s", id, rang))
							wait(1500); sampSendChat("/me потушил экран планшета и убрал его обратно.")
						else
							sampSendChat("/me из внутреннего кармана достала планшет и включила его.")
							wait(1500); sampSendChat("/me зайдя в базу данных, изменила должность " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
							wait(1000); sampSendChat(string.format("/rang %d %s", id, rang))
							wait(1500); sampSendChat("/me потушила экран планшета и убрала его обратно.")
						end
					end)
				else
					chat("Данный игрок находится слишком далеко от Вас.")
				end
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
			end
		else
			chat_error("Введите необходимые параметры для /rang [ид игрока] [+ или -].")
		end
	else
		chat("Вы не можете использовать данную функцию, вы не лидер и не его заместитель.")
	end
end

function command_changeskin(id)
	if player_status > 0 then
		if string.match(id, "(%d+)") then
			if isPlayerConnected(id) then
				if sampGetDistanceToPlayer(id) < 3 then
					lua_thread.create(function()
						if not ini.info.male then
							sampSendChat("/me достал ключ от шкафчика под номером " .. id .. " и передал его " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
						else
							sampSendChat("/me достала ключ от шкафчика под номером " .. id .. " и передала его " .. string.gsub(sampGetPlayerName(id),"_"," " .. "."))
						end
						wait(1000); sampSendChat(string.format("/changeskin %d", id))
					end)
				else
					chat("Данный игрок находится слишком далеко от Вас.")
				end
			else
				chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
			end
		else
			chat_error("Введите необходимые параметры для /changeskin [ид игрока].")
		end
	else
		chat("Вы не можете использовать данную функцию, вы не лидер и не его заместитель.")
	end
end

function command_cuff(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 3 then
				lua_thread.create(function()
					local acting = {
						[0] = {
							[1] = {
								"/me завёл руки нарушителя за спину, после чего растягнул чехол для наручников.",
								"wait: 1500",
								"/me достав наручники из чехла, застегнул их на запястьях преступника."
							},
							[2] = {
								"/me удерживая подозреваемого, растягнул один из чехлов на тактическом поясе.",
								"wait: 1500",
								"/me достал из чехла наручники и надел их на запастья подозреваемого."
							}
						},
						[1] = {
							[1] = {
								"/me завела руки нарушителя за спину, после чего растягнула чехол для наручников.",
								"wait: 1500",
								"me достав наручники из чехла, застегнула их на запястьях преступника."
							},
							[2] = {
								"/me удерживая подозреваемого, растягнула один из чехлов на тактическом поясе.",
								"wait: 1500",
								"/me достала из чехла наручники и надела их на запастья подозреваемого."
							}
						}
					}
					variable_acting_out(acting, 1, 2)
					wait(1000); sampSendChat(string.format("/cuff %d", id))
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /cuff [ид игрока].")
	end
end

function command_uncuff(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 3 then
				lua_thread.create(function()
					sampSendChat("/do На запястьях " .. string.gsub(sampGetPlayerName(id),"_"," ") .. " находятся наручники."); wait(1500)
					if not ini.info.male then
						sampSendChat("/me сняв с пояса связку ключей, провернул один из них в замке наручников."); wait(1000)
						sampSendChat(string.format("/uncuff %d", id)); wait(1500)
						sampSendChat("/me убрав наручники в чехол, повесил связку ключей на пояс.")
					else
						sampSendChat("/me сняв с пояса связку ключей, провернула один из них в замке наручников."); wait(1000)
						sampSendChat(string.format("/uncuff %d", id)); wait(1500)
						sampSendChat("/me убрав наручники в чехол, повесила связку ключей на пояс.")
					end
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /uncuff [ид игрока].")
	end
end

function command_skip(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 5 then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me из внутреннего кармана достал планшет и включил его.")
						wait(1500); sampSendChat("/me зайдя в базу данных, оформил временный пропуск на имя " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
						wait(1000); sampSendChat(string.format("/skip %d", id))
						wait(1500); sampSendChat("/me потушил экран планшета и убрал его обратно.")
						wait(1000); command_r("Оформил временный пропуск на имя " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
					else
						sampSendChat("/me из внутреннего кармана достала планшет и включила его.")
						wait(1500); sampSendChat("/me зайдя в базу данных, оформила временный пропуск на имя " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
						wait(1000); sampSendChat(string.format("/skip %d", id))
						wait(1500); sampSendChat("/me потушила экран планшета и убрала его обратно.")
						wait(1000); command_r("Оформила временный пропуск на имя " .. string.gsub(sampGetPlayerName(id),"_"," ") .. ".")
					end
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /skip [ид игрока].")
	end
end

function command_upopen()
	lua_thread.create(function()
		sampSendChat("/open")
		openBarrier = true
		wait(150)
		openBarrier = false
	end)
end

function command_gate()
	lua_thread.create(function()
		sampSendChat("/gate")
		openBarrier = true
		wait(150)
		openBarrier = false
	end)
end

function command_clear(param)
	if string.match(param, "(%d+) (%S+)") then
		local id, reason = string.match(param, "(%d+) (.+)")
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 10 then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me достав планшет, зашёл в базу данных и нашёл нужное дело.")
						wait(1500); sampSendChat("/me пролистав страницу в самый низ, выбрал пункт с закрытием дела.")
						wait(1000); sampSendChat(string.format("/clear %d" , id))
						wait(1500); command_f("Дело №" .. id .. " (" .. string.gsub(sampGetPlayerName(id),"_"," ") .. ") закрыто. Причина: " .. reason .. ".")
						wait(1500); sampSendChat("/me потушил экран планшета и убрал его обратно.")
					else
						sampSendChat("/me достав планшет, зашла в базу данных и нашла нужное дело.")
						wait(1500); sampSendChat("/me пролистав страницу в самый низ, выбрала пункт с закрытием дела.")
						wait(1000); sampSendChat(string.format("/clear %d" , id))
						wait(1500); command_f("Дело №" .. id .. " (" .. string.gsub(sampGetPlayerName(id),"_"," ") .. ") закрыто. Причина: " .. reason .. ".")
						wait(1500); sampSendChat("/me потушила экран планшета и убрала его обратно.")
					end
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /clear [ид игрока] [причина].")
	end
end

function command_putpl(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 4 then
				lua_thread.create(function()
					local acting = {
						[0] = {
							[1] = {"/me подведя подозреваемого к автомобилю, открыл дверь и посадил его туда."},
							[2] = {
								"/me удерживая подозреваемого, свободной рукой открыл дверь в патрульном автомобиле.",
								"wait: 1000",
								"/me пригнул голову подозреваемого и усадил его в машину, закрыл за ним дверь."
							}
						},
						[1] = {
							[1] = {"/me подведя подозреваемого к автомобилю, открыла дверь и посадила его туда."},
							[2] = {
								"/me удерживая подозреваемого, свободной рукой открыла дверь в патрульном автомобиле.",
								"wait: 1000",
								"/me пригнула голову подозреваемого и усадила его в машину, закрыла за ним дверь."
							}
						}
					}
					variable_acting_out(acting, 1, 2)
					wait(1000); sampSendChat(string.format("/putpl %d", id))
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /putpl [ид игрока].")
	end
end

function command_hold(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 3 then
				lua_thread.create(function()
					local acting = {
						[0] = {
							[1] = {"/me заломав руку подозреваемого, повёл его за собой."},
							[2] = {"/me крепко схватил подозреваемого и потащил его за собой."}
						},
						[1] = {
							[1] = {"/me заломав руку подозреваемого, повела его за собой."},
							[2] = {"/me крепко схватила подозреваемого и потащила его за собой."}
						}
					}
					variable_acting_out(acting, 1, 2)
					wait(1000); sampSendChat(string.format("/hold %d", id))
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /hold [ид игрока].")
	end
end

function command_ticket(param)
	if string.match(param, "(%d+) (%d+) (%S+)") then
		local id, money, reason = string.match(param, "(%d+) (%d+) (.+)")
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 5 then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me достал блокнот, ручку и начал записывать информацию о нарушении.")
						wait(1500); sampSendChat("/me заполнив всю информацию о нарушении, передал бланк нарушителю.")
						wait(1000); sampSendChat(string.format("/ticket %d %d %s", id, money, reason))
						wait(1500); sampSendChat("/me убрал блокнот и ручку обратно во внутренний карман.")
					else
						sampSendChat("/me достала блокнот, ручку и начала записывать информацию о нарушении.")
						wait(1500); sampSendChat("/me заполнив всю информацию о нарушении, передала бланк нарушителю.")
						wait(1000); sampSendChat(string.format("/ticket %d %d %s", id, money, reason))
						wait(1500); sampSendChat("/me убрала блокнот и ручку обратно во внутренний карман.")
					end
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	elseif string.match(param, "(%d+)") then
		local id = string.match(param, "(%d+)")
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) <= 5 then
				if smart_ticket.text then
					smart_ticket.id = id
					show_ticket.v = not show_ticket.v
				else
					chat("Данная возможность Вам недоступна.")
				end
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /ticket [ид игрока] [сумма] [причина].")
	end
end

function command_takelic(param)
	if string.match(param, "(%d+) (%S+)") then
		local id, reason = string.match(param, "(%d+) (.+)")
		if isPlayerConnected(id) then
			local name = sampGetPlayerName(id)
			if userList[name] then
				if userList[name].rangNumber > 2 then
					if player_status ~= 4 then
						chat(("Вы не можете изъять лицензии у пользователя %s%s[%d]{}."):format(userList[name].color, name, id))
						return
					end
				end
			end
		
			if sampGetDistanceToPlayer(id) < 15 then
				lua_thread.create(function()
					if not ini.info.male then
						sampSendChat("/me достал планшет, зашёл в базу данных и ввёл номер Т/С.")
						wait(1500); sampSendChat("/me получив информацию о водителе, изъял его лицензию на вождение.")
					else
						sampSendChat("/me достала планшет, зашла в базу данных и ввела номер Т/С.")
						wait(1500); sampSendChat("/me получив информацию о водителе, изъяла его лицензию на вождение.")
					end
					wait(1000); sampSendChat(string.format("/takelic %d %s", id, reason))
				end)
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /takelic [ид игрока] [причина].")
	end
end

function command_pull(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 3 then
				local res, vid = sampIsPlayerCar(id)
				if res then
					lua_thread.create(function()
						if tCarsType[vid - 399] == 2 or tCarsType[vid - 399] == 9 then
							if not ini.info.male then
								sampSendChat("/me подойдя к подозреваемому, толкнул его, тем самым скинув с байка.")
								wait(1000); sampSendChat(string.format("/pull %d", id))
							else
								sampSendChat("/me подойдя к подозреваемому, толкнула его, тем самым скинув с байка.")
								wait(1000); sampSendChat(string.format("/pull %d", id))
							end
						else
							local acting = {
								[0] = {
									[1] = {
										"/me сняв дубинку с пояса, ударил её тыльную частью по стеклу.",
										"wait: 1000",
										"/me повесив дубинку на пояс, просунул руку в стекло и открыл дверь.",
										"wait: 1000",
										"/me взявшись за подозреваемого, вытащил его из машины."
									},
									[2] = {
										"/me выхватил дубинку с тактического пояса и нанёс сильный удар по стеклу.",
										"wait: 1500",
										"/me открыл автомобильную дверь, схватился за одежду подозреваемого и вытащил его на землю."
									}
								},
								[1] = {
									[1] = {
										"/me сняв дубинку с пояса, ударила её тыльную частью по стеклу.",
										"wait: 1000",
										"/me повесив дубинку на пояс, просунула руку в стекло и открыла дверь.",
										"wait: 1000",
										"/me взявшись за подозреваемого, вытащила его из машины."
									},
									[2] = {
										"/me выхватила дубинку с тактического пояса и нанесла сильный удар по стеклу.",
										"wait: 1500",
										"/me открыла автомобильную дверь, схватилась за одежду подозреваемого и вытащила его на землю."
									}
								}
							}
							variable_acting_out(acting, 1, 2)
							wait(1000); sampSendChat(string.format("/pull %d", id))
						end
					end)
				else
					chat("Данный игрок не находится в автомобиле.")
				end
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /pull [ид игрока].")
	end
end

function command_arrest(id)
	if string.match(id, "(%d+)") then
		if isPlayerConnected(id) then
			if sampGetDistanceToPlayer(id) < 5 then
				if sampIsPlayerCarHaveSuspect(id) then
					lua_thread.create(function()
						local acting = {
							[0] = {
								[1] = {
									"/me включил бортовой компьютер и заполнил информацию о нарушителе.",
									"wait: 1500",
									"/me снял рацию с плеча, поднёс её ко рту и что-то произнёс.",
									"wait: 1500",
									"/do Из департамента вышли два офицера и забрали подозреваемого с собой."
								},
								[2] = {
									"/me снял тангету с плеча, зажал кнопку PTT и передал информацию о подозреваемом диспетчеру.",
									"wait: 1500",
									"/do Из департамента вышли два офицера и забрали подозреваемого с собой."
								}
							},
							[1] = {
								[1] = {
									"/me включила бортовой компьютер и заполнила информацию о нарушителе.",
									"wait: 1500",
									"/me сняла рацию с плеча, поднесла её ко рту и что-то произнесла.",
									"wait: 1500",
									"/do Из департамента вышли два офицера и забрали подозреваемого с собой."
								},
								[2] = {
									"/me сняла тангету с плеча, зажала кнопку PTT и передала информацию о подозреваемом диспетчеру.",
									"wait: 1500",
									"/do Из департамента вышли два офицера и забрали подозреваемого с собой."
								}
							}
						}
						variable_acting_out(acting, 1, 2)
					
						command_r(("Подозреваемый по делу №%d (%s) был передан в департамент."):format(id, string.gsub(sampGetPlayerName(id),"_"," "))); wait(1000)
						sampSendChat(string.format("/arrest %d", id))
					end)
				else
					chat("В вашей машине не находится данный игрок.")
				end
			else
				chat("Данный игрок находится слишком далеко от Вас.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /arrest [ид игрока].")
	end
end

function command_su(param)
	if string.match(param, "(%d+) (%d+) (%S+)") then
		local id, stars, reason = string.match(param, "(%d+) (%d+) (.+)")
		if isPlayerConnected(id) then
			local name = sampGetPlayerName(id)
			if userList[name] then
				if userList[name].rangNumber > 2 then
					if player_status ~= 4 then
						chat(("Вы не можете объявить в розыск пользователя %s%s[%d]{}."):format(userList[name].color, name, id))
						return
					end
				end
			end
		
		
			if sampGetPlayerColor(id) ~= 2236962 then
				lua_thread.create(function()
					if sampGetDistanceToPlayer(id) <= 65 then
						local acting = {
							[0] = {
								[1] = {"/me сняв тангету с плеча, передал диспетчеру информацию о подозреваемом."},
								[2] = {"/me снял тангету с плеча, зажал кнопку PTT и передал информацию о подозреваемом диспетчеру."}
							},
							[1] = {
								[1] = {"/me сняв тангету с плеча, передала диспетчеру информацию о подозреваемом."},
								[2] = {"/me сняла тангету с плеча, зажала кнопку PTT и передала информацию о подозреваемом диспетчеру."}
							}
						}
						variable_acting_out(acting, 1, 2)
						
						wait(1000)
						
						if sampGetDistanceToPlayer(id) <= 65 then
							sampSendChat(string.format("/su %d %d %s", id, stars, reason))
						else
							chat("Подозреваемый слишком далеко, розыск выдастся сразу же, как будет возможность.")
							while sampGetDistanceToPlayer(id) > 65 do
								wait(0)
							
								local result, ped = sampGetCharHandleBySampPlayerId(id)
								if not result then 
									chat("Подозревамый пропал из зоны видимости.")
									break
								elseif sampGetPlayerColor(id) == 2236962  then
									chat("Подозреваемый надел на лицо маску, невозможно выдать розыск.")
									break
								end
							end
							
							if sampGetDistanceToPlayer(id) <= 65 then
								sampSendChat(string.format("/su %d %d %s", id, stars, reason))
							end
						end
					else
						chat("Данный игрок находится слишком далеко от Вас.")
					end
				end)
			else
				chat("Данный игрок находится в маске.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	elseif string.match(param, "(%d+)") then
		local id = string.match(param, "(%d+)")
		if isPlayerConnected(id) then
			if sampGetPlayerColor(id) ~= 2236962 then
				if sampGetDistanceToPlayer(id) <= 65 then
					if smart_suspect.text then
						smart_suspect.id = id
						show_suspect.v = not show_suspect.v
					else
						chat("Данная возможность Вам недоступна.")
					end
				else
					chat("Данный игрок находится слишком далеко от Вас.")
				end
			else
				chat("Данный игрок находится в маске.")
			end
		else
			chat("Данный игрок не подключён к серверу. Проверьте правильность введёного ID.")
		end
	else
		chat_error("Введите необходимые параметры для /su [ид игрока] [розыск] [причина].")
	end
end

function command_admins()
	if player_status > 1 then
		if not adminList then
			local path = "moonloader//helper//admins.txt"
			adminList = {}
			
			if doesFileExist(path) then
				for line in io.lines(path) do
					if string.match(line, "(%S+) | (.+) | (%d+)") then
						local name, nameRang, rang = string.match(line, "(%S+) | (.+) | (%d+)")
						local input = {name = name, nameRang = nameRang, rang = rang}
						table.insert(adminList, input)
					end
				end
				
				sampShowDialog(1001, "{FFCD00}Список администрации", "{e6e6fa}Список администрации был успешно сформирован.\nВведите команду {00CC66}/admins{e6e6fa} ещё раз, чтобы открыть список.", "Закрыть", "", 0)
			else
				sampShowDialog(1001, "{FFCD00}Список администрации", "{e6e6fa}Невозможно обновить список в связи с тем, что {00CC66}файл конфигурации{e6e6fa} отсутствует", "Закрыть", "", 0)
			end
		else
			local dline = "Имя\tДолжность\tСтатус\n"
			local all = 0
			local online = 0
		
			for index, value in pairs(adminList) do
				all = all + 1
				local id = sampGetPlayerIdByNickname(value.name)
				
				if id == "OFF" then
					dline = string.format("%s{696969}%s\t{696969}%d ранг. %s\t{ff5c33}Оффлайн\n", dline, value.name, value.rang, value.nameRang)
				else
					online = online + 1
					dline = string.format("%s{%s}%s[%d]\t%d ранг. %s\t{00cc99}Онлайн\n", dline, sampGetColorByPlayer(id), value.name, id, value.rang, value.nameRang)
				end
			end
			
			local caption = string.format("{e6e6fa}Всего пользователей {FFCD00}%d чел. {00CC66}(онлайн %d)", all, online)
			sampShowDialog(1001, caption, dline, "Закрыть", "", 5)
		end
	end
end

function command_users()
	if player_status > 1 then
		local result = https.request("https://raw.githubusercontent.com/skezz-perry/project/master/users")
		local result = decodeJson(result)

		if result and type(result) == "table" then
			userList = result
			local dline = "Имя\tСтатус\tДата окончания подписки\tСтатус подписки\n"
			local all = 0
			local online = 0 
			
			for number = 4, 1, -1 do
				for index, value in pairs(userList) do
					if value.rangNumber == number then
						local id = sampGetPlayerIdByNickname(index)
						local rang = ("%s%s"):format(value.color, u8:decode(value.rang))
						local status = (checkValiditySubscription(value.account) and string.format("%s\t{00cc99}Дейсвительна", value.account) or string.format("%s\t{ff5c33}Просрочена", value.account))
						if id == "OFF" then
							dline = string.format("%s{696969}%s\t{696969}%s ранг. %s\t%s\n", dline, index, value.rangNumber, rang, status)
						else
							online = online + 1
							dline = string.format("%s{%s}%s[%d]\t%s ранг. %s\t%s\n", dline, sampGetColorByPlayer(id), index, id, value.rangNumber, rang, status)
						end
						all = all + 1 
					end  
				end
			end
			
			local caption = string.format("{e6e6fa}Всего пользователей {FFCD00}%d чел. {00CC66}(онлайн %d)", all, online)
			sampShowDialog(1001, caption, dline, "Закрыть", "", 5)
		else
			chat("Произошла ошибка при попытке получить информацию о пользователях.")
		end
	end
end

--[[function command_bl(param)
	if string.match(param, "(%d+)") then
		if isPlayerConnected(param) then
			local name = sampGetPlayerName(param)
			sampSendChat("/history " .. name)
